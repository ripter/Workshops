function update_collisions()
	-- is a wall in range to be hit?
	if did_hit(wall1.x, player.x) then
		print('üê±', 8, 8)
	elseif did_hit(wall2.x, player.x) then
		print('‚òâ', 8, 8)
	else
--		print(player.x, 8, 8)
		print(wall1.x, 20, 8)
		print(wall2.x, 40, 8)
	end
end


function did_hit(x1, x2) 
	return x1 <= x2+1 and x1 >= x2-1
end
